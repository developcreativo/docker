{
  "name": "laradocker",
  "dockerComposeFile": "../docker-compose.yml",
  "remoteUser": "laradock",
  "runServices": [
    "nginx",
    "workspace",
    "mysql",
    "phpmyadmin",
    "mailhog",
    "php-worker",
    "redis",
    "portainer"
  ],
  "service": "workspace",
  "workspaceFolder": "/var/www",
  "shutdownAction": "stopCompose",
"postCreateCommand": "uname -a",
"customizations": {
	"vscode": {
		"extensions": [
			"patbenatar.advanced-new-file",
			"PhilHindle.errorlens",
			"dbaeumer.vscode-eslint",
			"wix.vscode-import-cost",
			"shevaua.phpcs",
			"wware.snippet-creator",
			"formulahendry.auto-close-tag",
			"formulahendry.auto-rename-tag",
			"njpwerner.autodocstring",
			"mgmcdermott.vscode-language-babel",
			"alefragnani.Bookmarks",
			"caomeinaixi.create-component",
			"dsznajder.es7-react-js-snippets",
			"Serhioromano.vscode-gitflow",
			"mhutchie.git-graph",
			"eamodio.gitlens",
			"atlassian.atlascode",
			"ctf0.laravel-goto-config",
			"ctf0.laravel-goto-controller",
			"ryannaddy.laravel-artisan",
			"recca0120.vscode-phpunit",
			"ms-python.python",
			"ms-python.vscode-pylance",
			"dacoto.laravel-easy-blade-snippets",
			"shufo.vscode-blade-formatter",
			"amiralizadeh9480.laravel-extra-intellisense",
			"ctf0.laravel-goto-env",
			"ctf0.laravel-goto-lang",
			"ctf0.laravel-goto-path",
			"ctf0.laravel-goto-view",
			"mintlify.document",
			"bmewburn.vscode-intelephense-client",
			"octref.vetur",
			"batisteo.vscode-django",
			"ms-azuretools.vscode-docker",
			"Vue.volar",
			"ClickCorp.clicknium",
			"ms-toolsai.vscode-jupyter-slideshow",
			"ms-toolsai.jupyter-renderers",
			"ms-toolsai.vscode-jupyter-cell-tags",
			"ms-toolsai.jupyter",
			"alexcvzz.vscode-sqlite",
			"hbenl.vscode-test-explorer",
			"redhat.vscode-yaml"
		]
	}
}
}
