{
  "name": "laradocker",
  "dockerComposeFile": "../docker-compose.yml",
  "remoteUser": "laradock",
  "runServices": [
    "nginx",
    "mysql",
    "phpmyadmin"
  ],
  "service": "workspace",
  "workspaceFolder": "/var/www",
  "shutdownAction": "stopCompose",
"postCreateCommand": "uname -a",
"customizations": {
	"vscode": {
		"extensions": [
			"mgmcdermott.vscode-language-babel",
			"caomeinaixi.create-component",
			"ctf0.env-symbol-provider",
			"Vue.volar",
			"Vue.vscode-typescript-vue-plugin",
			"hbenl.vscode-test-explorer",
			"ms-vscode.test-adapter-converter",
			"bradlc.vscode-tailwindcss",
			"ms-python.python",
			"esbenp.prettier-vscode",
			"ms-python.vscode-pylance",
			"recca0120.vscode-phpunit",
			"shevaua.phpcs",
			"ctf0.php-array-symbols",
			"ctf0.laravel-goto-view",
			"ctf0.laravel-goto-path",
			"ctf0.laravel-goto-lang",
			"ctf0.laravel-goto-env",
			"ctf0.laravel-goto-controller",
			"ctf0.laravel-goto-config",
			"amiralizadeh9480.laravel-extra-intellisense",
			"ryannaddy.laravel-artisan",
			"ms-toolsai.vscode-jupyter-slideshow",
			"ms-toolsai.jupyter-renderers",
			"ms-toolsai.vscode-jupyter-cell-tags",
			"ms-toolsai.jupyter",
			"ms-python.isort",
			"mhutchie.git-graph",
			"ms-azuretools.vscode-docker",
			"batisteo.vscode-django",
			"mintlify.document",
			"octref.vetur",
			"damianbal.vs-phpclassgen",
			"sevavietl.php-files"
		]
	}
}
}
